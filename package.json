    "prettier": "^3.3.3",
    "tsc": "^2.0.4"
  },
  "peerDependencies": {
    "typescript": "5.0.0"
  },
  "dependencies": {
    "@sparticuz/chromium": "^126.0.0",
    "colors": "^1.4.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "puppeteer": "^22.13.1",
    "puppeteer-core": "^22.13.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "puppeteer-intercept-and-modify-requests": "^1.3.1"
  }
