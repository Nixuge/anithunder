{
  "name": "aniwave-keys",
  "description": "Extracts keys from Aniwave",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run dist/index.js",
    "dev": "bun src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "build:db": "bun run src/scripts/buildDb.ts",
